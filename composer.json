{
    "name": "brianvb/yii2-fontawesome",
    "description": "Asset bundle for font awesome using composer",
    "type": "yii2-extension",
    "require": {
        "fortawesome/font-awesome": "@dev",
        "yiisoft/yii2": "^2.0"
    },
    "authors": [
        {
            "name": "BrianVB",
            "email": "vanburen.brian@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "bvb\\fontawesome\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
